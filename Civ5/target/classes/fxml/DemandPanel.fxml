<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="450.0" prefWidth="800.0" stylesheets="@../css/DiplomacyPanel.css" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View.Controller.DemandPanelController">
   <children>
      <VBox fx:id="list1" layoutX="405.0" layoutY="56.0" prefHeight="322.0" prefWidth="150.0" spacing="5.0" />
      <Button fx:id="exit" layoutX="14.0" layoutY="384.0" mnemonicParsing="false" prefHeight="50.0" prefWidth="94.0" text="Exit" textFill="WHITE">
         <font>
            <Font size="22.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <VBox fx:id="list2" layoutX="570.0" layoutY="56.0" prefHeight="322.0" prefWidth="94.0" />
      <Button layoutX="621.0" layoutY="384.0" mnemonicParsing="false" onMouseClicked="#demand" prefHeight="50.0" prefWidth="165.0" text="Demand" textFill="WHITE">
         <font>
            <Font size="22.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Label fx:id="error" alignment="CENTER" layoutX="392.0" layoutY="392.0" prefHeight="34.0" prefWidth="228.0" text="Enter valid numbers" textFill="RED" visible="false">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <Label fx:id="choose" layoutX="411.0" layoutY="22.0" prefHeight="34.0" prefWidth="254.0" text="Choose product for demand" textFill="WHITE">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
